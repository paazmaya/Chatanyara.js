{
  "name": "chatanyara.js",
  "version": "0.2.0",
  "description": "Collect Performance API data to a plain JavaScript Object",
  "homepage": "https://github.com/paazmaya/chatanyara.js",
  "author": {
    "name": "Juga Paazmaya",
    "email": "paazmaya@yahoo.com",
    "url": "http://paazmaya.fi"
  },
  "main": "chatanyara.js",
  "scripts": {
    "test": "npm run lint && mocha-phantomjs -p node_modules/phantomjs/bin/phantomjs tests/index.html",
    "lint": "eslint -f compact -c .eslintrc.json chatanyara.js",
    "build": "uglifyjs --compress --output chatanyara.min.js --comments --stats chatanyara.js"
  },
  "files": [
    "chatanyara.js",
    "chatanyara.min.js",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/paazmaya/chatanyara.js.git"
  },
  "keywords": [
    "performance",
    "navigation",
    "timing",
    "resource"
  ],
  "testling": {
    "browsers": [
      "ie/8..10",
      "firefox/3.5",
      "firefox/latest",
      "chrome/latest",
      "safari/5..latest",
      "opera/11",
      "opera/next"
    ],
    "html": "tests/index.html"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paazmaya/chatanyara.js/issues"
  },
  "devDependencies": {
    "chai": "~3.4",
    "eslint": "~1.10",
    "mocha": "~2.3",
    "mocha-phantomjs": "~4.0",
    "uglifyjs": "~2.4"
  }
}
